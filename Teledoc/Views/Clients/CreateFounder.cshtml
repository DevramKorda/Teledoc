@model Teledoc.Models.Founder

@{
    ViewData["Title"] = "Founder Create";
}

<form asp-action="CreateFounder">
    <div class="col-md-4">
        <div>
            <label asp-for="Inn" class="control-label"></label>
            <input asp-for="Inn" class="form-control" />
            <span asp-validation-for="Inn"></span>
        </div>

        <div>
            <label asp-for="Fio" class="control-label"></label>
            <input asp-for="Fio" class="form-control" />
            <span asp-validation-for="Fio"></span>
        </div>

        <div>
            <input type="submit" value="save" />
        </div>
    </div>
</form>

@*для валидации*@
@section scripts{
    <script src="~/lib/jquery-validation/dist/jquery.validate.min.js"></script>
    <script src="~/lib/jquery-validation-unobtrusive/jquery.validate.unobtrusive.min.js"></script>
}
